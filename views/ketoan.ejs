<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Kế toán</title>
    <!-- Latest compiled and minified CSS -->

</head>
<body>

<section class="main col-md-12">
    <div class="container">
        <div class="grid">
            <div class="row">
                <% for(var i=0;i<product.length;i++){ %>
                    <div class="item">
                        <a href="/chi-tiet?id=<%= product[i].id %>">
                            <img src="<%= product[i].image %>"/>
                        </a>
                        <a href="/chi-tiet2/<%= products[i].id %>">
                            <h2><%= product[i].name %></h2>
                        </a>
                    </div>
                <% } %>


            </div>

        </div>
    </div>
</section>
</body>
</html>